import java.util.*;

public class Launch {

	public static void main(String[] args) {
		Person p1= new Person();
		p1.setName("Aman");
		System.out.println(p1.getName());
	}

}

class Person {
	private String name;
	
	public void setName(String name) {
		this.name=name;
	}
	public String getName() {
		return name;
	}
}
